{
  "name": "discord-source",
  "private": true,
  "version": "0.0.13",
  "scripts": {
    "vite": "vite",
    "vite:dev": "vite",
    "vite:build": "vue-tsc --noEmit && vite build",
    "discord:vite:watch": "vite build --watch --mode discord",
    "discord:vite:build": "vite build --mode discord",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build-bindings": "cargo test export_bindings --manifest-path src-tauri/Cargo.toml",
    "bundlebd": "bundlebd",
    "bundlebd:build": "bundlebd",
    "bundlebd:dev": "bundlebd --dev",
    "nv": "node --version",
    "setVersion": "ts-node ./scripts/setVersion.ts",
    "prepareDev": "ts-node ./scripts/prepareDev.ts",
    "prepareBuild": "ts-node ./scripts/prepareBuild.ts",
    "clean": "rimraf node_modules dist-bd dist src-tauri/dist src-tauri/target src-tauri/bindings",
    "postinstall": "mkdirp ./src-tauri/dist/web/ && nodetouch ./src-tauri/dist/web/index.html && mkdirp ./dist/ && mkdirp ./dist-bd/ && nodetouch ./dist-bd/DiscordSourcePlugin.plugin.js"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@tauri-apps/api": "2.0.0-alpha.3",
    "roboto-fontface": "^0.10.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.15",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@tauri-apps/cli": "2.0.0-alpha.8",
    "@types/node": "^18.16.1",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vueuse/core": "^9.13.0",
    "bundlebd": "^4.0.0",
    "deepmerge": "^4.3.1",
    "esbuild": "^0.17.18",
    "material-design-icons-iconfont": "^6.7.0",
    "mkdirp": "^2.1.6",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.0",
    "sass": "^1.62.1",
    "touch": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-event-target": "^1.0.4",
    "vite": "^4.3.2",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.4.4"
  }
}
